{
    "name": "punksolid/doopla",
    "description": "PHP extractor library for doopla.mx",
    "type": "library",
    "require": {
        "php": "^8.2",
        "symfony/http-client": "^7.0",
        "symfony/dom-crawler": "^7.0",
        "symfony/css-selector": "^7.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.0",
        "phpunit/phpunit": "^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Punksolid\\Doopla\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Punksolid\\Doopla\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jose. Palazuelos",
            "email": "punksolid@gmail.com"
        }
    ]
